<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dutu.admin.mapper.BtMapper" >
    <insert id="insertAndGetId" useGeneratedKeys="true" keyProperty="btid" parameterType="com.dutu.admin.bean.Bt">
     insert into bt(btid,bt)
        values(#{btid},#{bt})
    </insert>

</mapper>